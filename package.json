{
   "name": "gator-data",
   "version": "1.0.0",
   "homepage" : "http://hirenj.github.com/gator-data/",
   "description": "GATOR web server",
   "keywords": [
       "bioinformatics",
       "node",
       "javascript" 
   ],
   "maintainers": [
       {
           "name": "Hiren Joshi",
           "email": "",
           "web": "" 
       } 
   ],
   "licenses": [
       {
           "type": "GPLv2",
           "url": "http://www.example.org/licenses/gpl.html" 
       } 
   ],
   "repositories": [
       {
           "type": "git",
           "url": "https://hirenj@github.com/hirenj/gator-data.git" 
       } 
   ],
   "dependencies": {
       "express" : "2.4.x",
       "optimist": "0.2.x",
